        }
    },
    "authors": [
        {
            "name": "DesenvolvimentoEasingJob",
            "email": "desenvolvimentoeasingjob@easingjob.com.br"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
